{
  "expo": {
    "name": "To-do-app",
    "slug": "To-do-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mainIcon.png",
    "splash": {
      "image": "./assets/mainIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jongjinjung.todoapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/mainIcon.png"
    }
  }
}
